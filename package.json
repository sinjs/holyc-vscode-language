{
  "name": "holyc",
  "displayName": "HolyC",
  "icon": "./assets/HolyC.png",
  "description": "Language Support for the HolyC Language by Terry Davis",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sinjs/holyc-vscode-language"
  },
  "publisher": "sinjs",
  "contributes": {
    "languages": [
      {
        "id": "holyc",
        "aliases": [
          "HolyC",
          "holyc"
        ],
        "extensions": [
          ".hc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "holyc",
        "scopeName": "source.hc",
        "path": "./syntaxes/holyc.tmLanguage.json"
      }
    ]
  }
}
